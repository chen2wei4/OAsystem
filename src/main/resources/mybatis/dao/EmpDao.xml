<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.woniuxy.oasystem.dao.EmpDao">
  	<select id="getEmpId" resultType="Emp">
  		select * from emp where emp_id=#{empId}
  	</select>
  	<select id="getEmpEmail" resultType="Emp">
  		select * from emp where emp_email=#{empEmail}
  	</select>
  	<select id="getEmpCellNumber" resultType="Emp">
  		select * from emp where emp_tel=#{empTel}
  	</select>
  </mapper>